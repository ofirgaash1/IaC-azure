{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "israelcentral"
    },
    "tagEnvironment": {
      "value": "dev"
    },
    "tagOwner": {
      "value": "test"
    },
    "startupScript": {
      "value": ""
    },
    "networkSecurityGroupName": {
      "value": "ofirVM-nsg"
    },
    "networkSecurityGroupRules": {
      "value": [
        {
          "name": "SSH",
          "properties": {
            "priority": 300,
            "protocol": "TCP",
            "access": "Allow",
            "direction": "Inbound",
            "sourceAddressPrefix": "*",
            "sourcePortRange": "*",
            "destinationAddressPrefix": "*",
            "destinationPortRange": "22"
          }
        },
        {
          "name": "HTTPS",
          "properties": {
            "priority": 320,
            "protocol": "TCP",
            "access": "Allow",
            "direction": "Inbound",
            "sourceAddressPrefix": "*",
            "sourcePortRange": "*",
            "destinationAddressPrefix": "*",
            "destinationPortRange": "443"
          }
        },
        {
          "name": "HTTP",
          "properties": {
            "priority": 340,
            "protocol": "TCP",
            "access": "Allow",
            "direction": "Inbound",
            "sourceAddressPrefix": "*",
            "sourcePortRange": "*",
            "destinationAddressPrefix": "*",
            "destinationPortRange": "80"
          }
        }
      ]
    },
    "subnetName": {
      "value": "default"
    },
    "virtualNetworkName": {
      "value": "ofirVM-vnet"
    },
    "addressPrefixes": {
      "value": [
        "10.0.0.0/16"
      ]
    },
    "subnets": {
      "value": [
        {
          "name": "default",
          "properties": {
            "addressPrefix": "10.0.0.0/24"
          }
        }
      ]
    },
    "publicIpAddressName": {
      "value": "ofirVM-ip"
    },
    "publicIpAddressType": {
      "value": "Static"
    },
    "publicIpAddressSku": {
      "value": "Standard"
    },
    "virtualMachineName": {
      "value": "ofirVM"
    },
    "virtualMachineComputerName": {
      "value": "ofirVM"
    },
    "virtualMachineRG": {
      "value": "Ofir-RG"
    },
    "osDiskType": {
      "value": "Premium_LRS"
    },
    "osDiskDeleteOption": {
      "value": "Delete"
    },
    "virtualMachineSize": {
      "value": "Standard_D2s_v3"
    },
    "networkInterfaceName": {
      "value": "ofirvm487"
    },
    "enableAcceleratedNetworking": {
      "value": true
    },
    "nicDeleteOption": {
      "value": "Detach"
    },
    "hibernationEnabled": {
      "value": false
    },
    "adminUsername": {
      "value": "ofir"
    },
    "adminPublicKey": {
      "value": "ssh-rsa  ofir@ofir-Latitude-7420"
    },
    "enablePeriodicAssessment": {
      "value": "ImageDefault"
    },
    "patchMode": {
      "value": "AutomaticByPlatform"
    },
    "rebootSetting": {
      "value": "IfRequired"
    },
    "securityType": {
      "value": "TrustedLaunch"
    },
    "secureBoot": {
      "value": true
    },
    "vTPM": {
      "value": true
    }
  }
}